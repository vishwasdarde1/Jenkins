pipeline{
    agent any
    stages {
        stage (level10) {
            steps {
                echo "hello, this is first jenkins pipeline, cloned from Github."
            }
        }
        stage (level20) {
            steps {
                echo "hello,Ok vishwas. is it correct?"
            }
        }
        stage (level30){
            steps{
                echo "Yes james, Do you have any sujjestions on this?"
            }
        }
        stage (level40){
            steps{
                echo "No vishwas, I was just asking, thank you."
            }
        }
    }
}
